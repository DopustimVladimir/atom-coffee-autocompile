{
  "name": "coffee-autocompile",
  "main": "./lib/coffee-autocompile",
  "version": "0.4.0",
  "description": "Auto compile Coffee-Script.",
  "repository": {
    "type": "git",
    "url": "https://github.com/olmokramer/atom-coffee-autocompile"
  },
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "activationCommands": {
    "atom-text-editor[data-grammar~=\"source\"][data-grammar~=\"coffee\"]": [
      "core:save"
    ]
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "dependencies": {
    "coffee-script": "^1.9.1",
    "loophole": "^1.1.0",
    "mkdirp": "latest",
    "uglify-js": "^2.5.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0"
  }
}
