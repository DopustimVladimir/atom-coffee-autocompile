{
  "name": "coffee-autocompile",
  "main": "./lib/coffee-autocompile",
  "version": "0.2.4",
  "description": "Auto compile Coffee-script.",
  "repository": {
    "type": "git",
    "url": "https://github.com/olmokramer/atom-coffee-autocompile"
  },
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "activationCommands": {
    "atom-text-editor[data-grammar~=\"source\"][data-grammar~=\"coffee\"]": [
      "core:save"
    ]
  },
  "dependencies": {
    "coffee-script": "^1.9.1",
    "mkdirp": "latest",
    "season": "^5.3.0",
    "uglify-js": "^2.4.16"
  }
}
